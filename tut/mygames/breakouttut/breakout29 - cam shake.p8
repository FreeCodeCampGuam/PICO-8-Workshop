pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
 -- timer
 t = 0
 
 -- score
 score = 0
 
 -- lives
 lives = 3

 -- paddle vars
 padx = 64 - 3*8/2
 pady = 120
 padspeed = 4
 
 -- ball vars
 make_ball()
 
 -- states
 paused = false
 over = false
 
 -- camera
 camx = 0
 camy = 0
 
 pause()
 music(0)
end

function make_ball()
 ballx = 60
 bally = 80
 ballspeed = .5
 balldx = rnd(ballspeed*2) - ballspeed + 1
 balldy = rnd(ballspeed) + 1 -- only down
end

function over_update()
 t += 1
 update_cam()
 if btn(4) then
 	-- reload the map
  reload()
  
  _update = game_update
  
  -- reset game
  _init()
 end
end

function pause_update()
 t += 1
 update_cam()
	if btnp(4) then
	 unpause()
	end
end

function game_update()
 t += 1
 -- move paddle
 if btn(0) then
  padx = max(8, padx - padspeed)
 end
 if btn(1) then
  padx = min(96, padx + padspeed)
 end
 
 
 -- check ball collisions
 
 -- check x. if moving by dx
 -- means we'll hit something
 -- make it bounce
 -- (go the other way)
 -- otherwise, keep going
 if collide(ballx+balldx, 
 										 bally) then
  balldx = -balldx
  camx = balldx*2
  sfx(0)
 else
  ballx += balldx
 end
 
 -- check y also
 if collide(ballx, 
 											bally+balldy) then
  balldy = -balldy
  camy = balldy*2
  sfx(0)
 elseif
   collide_paddle(ballx, bally+balldy)
 	 and balldy > 0 then
 	balldy *= -(rnd(.05)+1)
 	x_hit = (padx+12 - ballx)/12
 	balldx -= x_hit
 	sfx(0)
 else
  bally += balldy
 end
 
 -- lost a ball
 if bally >= 128 then
  sfx(2)
  lives -= 1
  camy = 4
  camx = rnd(4) -2
  if lives <= 0 then
   game_over()
  else
   pause()
  	make_ball()
  end
 end
 
 -- camera shakes
 update_cam()
 
end

function update_cam()
 camx *= .5
 camy *= .5
 if abs(camx) < .0005 then
  camx = 0
 end
end

function _draw()
	-- draw bg
	cls(13)
	
	-- camera
	camera(camx, camy)
	
	-- draw lvl
	map(0,0,0,0,16,16)
	
	-- draw paddle
	spr(10,padx,pady)
	spr(11,padx+8,pady)
	--right side• + 8 == 24
	spr(10,padx+16,pady,1,1,true)
	
	--draw ball
	spr(12,ballx,bally)
	
	--draw score
	print('score: '..score,16,2,5)
	print('score: '..score,16,1,7)
	
	--draw lives
	if (lives>=1) spr(13,128-16,0)
	if (lives>=2) spr(13,128-24,0)
	if (lives>=3) spr(13,128-32,0)
	
	--paused
	if paused and t%24>8 then
	 m = 'press z to unpause'
	 print(m, 64 - (#m * 4)/2, 
	 						101, 1)
	 print(m, 64 - (#m * 4)/2, 
	 						100, 7)
	end
	
	--game over
	if over then
	 m = 'game over!'
	 x = 64 - (#m * 4)/2
	 y = 40 + sin(t/60)*2
	 
	 --ribbon
	 line(0,y-5, t*30,y-5, 14)
	 rectfill(0,y-4, t*30+1,y+9, 8)
	 line(0,y+10, t*30,y+10, 2)
	 
	 --outline
	 print(m,x,y+2,14)
	 print(m,x+1,y,0)
	 print(m,x-1,y,0)
	 print(m,x,y+1,0)
	 print(m,x,y-1,0)
	 
	 print(m,x,y,7)
	 
	 --prompt
	 if t%24>8 then
		 m = 'press z to start'
		 print(m, 64 - (#m * 4)/2, 
		 						101, 1)
		 print(m, 64 - (#m * 4)/2, 
		 						100, 7)
		end
	end
end



-- x,y is in a solid object?
function solid(x,y)
 -- convert to map coords
 mx = flr(x/8)
 my = flr(y/8)
 
 -- get the sprite at x,y
 sprite = mget(mx, my)
 
 flag = fget(sprite, 0)
 if flag and fget(sprite, 1) then
   mset(mx,my,0)
   score += fget(sprite) + 1
 end
 return flag
end


-- check if a sprite is 
-- overlapping a solid object
function collide(x,y)
 -- if any of the corners
 -- are on a solid object
 -- we are colliding
 return 
 	solid(x  ,y  ) or
 	solid(x+8,y  ) or
 	solid(x  ,y+8) or
 	solid(x+8,y+8)
end

function collide_paddle(x,y)
 return  
 	hit_paddle(x  ,y  ) or
 	hit_paddle(x+8,y  ) or
 	hit_paddle(x  ,y+8) or
 	hit_paddle(x+8,y+8)
end

-- check if a x,y is within 
-- the paddle (collision)
--  ___  only care about
-- |   | ceiling and sides
--  () but if the ball is under
-- shouldn't count as a hit
function hit_paddle(x,y)
 -- ball bottom hit the ceiling
 ceil = pady
 floor = pady+8
 left = padx
 right = padx+8*3
 return 
  y > ceil and y < floor and
  x > left and x < right
end

-- pause/unpause
function pause()
 paused = true
 _update = pause_update
end

function unpause()
 paused = false
 _update = game_update
end

-- game over
function game_over()
 music(-1)
 sfx(1)
 t = 0
 over = true
 _update = over_update
end
